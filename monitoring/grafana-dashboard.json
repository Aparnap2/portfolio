{
  "dashboard": {
    "id": null,
    "title": "Audit Chatbot Production Dashboard",
    "tags": ["audit-chatbot", "production"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])",
            "legendFormat": "Error Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "max": 1,
            "min": 0
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 6,
          "h": 8
        }
      },
      {
        "id": 5,
        "title": "Audit Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "audit_sessions_total",
            "legendFormat": "Total Sessions"
          }
        ],
        "gridPos": {
          "x": 18,
          "y": 8,
          "w": 6,
          "h": 8
        }
      },
      {
        "id": 6,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "db_connections_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "db_connections_idle",
            "legendFormat": "Idle Connections"
          }
        ],
        "yAxes": [
          {
            "label": "Connections"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory Usage"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 8,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m])",
            "legendFormat": "CPU Usage"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "max": 1,
            "min": 0
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 9,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "cache_hits_total / (cache_hits_total + cache_misses_total)",
            "legendFormat": "Hit Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "max": 1,
            "min": 0
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 10,
        "title": "External API Calls",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(external_api_requests_total[5m])",
            "legendFormat": "{{service}} {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 32,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 11,
        "title": "Circuit Breaker Status",
        "type": "table",
        "targets": [
          {
            "expr": "circuit_breaker_state",
            "legendFormat": "{{service}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "CLOSED",
                    "color": "green"
                  },
                  "1": {
                    "text": "OPEN",
                    "color": "red"
                  },
                  "2": {
                    "text": "HALF_OPEN",
                    "color": "yellow"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "x": 12,
          "y": 32,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 14,
        "title": "Error Recovery Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(error_boundary_recovery_attempt[5m]) / rate(error_boundary_caught[5m])",
            "legendFormat": "Recovery Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "max": 1,
            "min": 0
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 40,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 15,
        "title": "Graceful Degradation Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(graceful_degradation_fallback[5m])",
            "legendFormat": "{{service}} Fallbacks"
          },
          {
            "expr": "rate(graceful_degradation_success[5m])",
            "legendFormat": "{{service}} Success"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 40,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 16,
        "title": "Retry Budget Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "retry_budget_exceeded",
            "legendFormat": "Budget Exceeded"
          }
        ],
        "yAxes": [
          {
            "label": "Count"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 48,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 17,
        "title": "Log Levels Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "increase(log_entries[1h])",
            "legendFormat": "{{level}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 48,
          "w": 12,
          "h": 8
        }
      }
      {
        "id": 12,
        "title": "Error Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "increase(error_count_total[1h])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 40,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 13,
        "title": "Health Check Status",
        "type": "stat",
        "targets": [
          {
            "expr": "health_check_status",
            "legendFormat": "{{check}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 40,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 16,
        "title": "Retry Budget Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "retry_budget_exceeded",
            "legendFormat": "Budget Exceeded"
          }
        ],
        "yAxes": [
          {
            "label": "Count"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 48,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 17,
        "title": "Log Levels Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "increase(log_entries[1h])",
            "legendFormat": "{{level}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 48,
          "w": 12,
          "h": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}